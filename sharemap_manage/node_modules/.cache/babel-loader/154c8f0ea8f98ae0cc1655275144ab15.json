{"ast":null,"code":"export default {\n  setup() {\n    const menulist = [{\n      icon: '',\n      name: 'User Management',\n      sub: [{\n        icon: '',\n        title: 'User List',\n        path: '/userlist'\n      }]\n    }, {\n      icon: '',\n      name: 'Visuals',\n      sub: [{\n        icon: '',\n        title: 'Visual Pages',\n        path: '/visualPage'\n      }, {\n        icon: '',\n        title: 'Visual Books',\n        path: '/visualBook'\n      }]\n    }, {\n      icon: '',\n      name: 'Clients',\n      sub: []\n    }];\n    const isCollapse = false;\n    const activePath = window.sessionStorage.getItem('activePath');\n\n    const logout = () => {\n      window.sessionStorage.clear();\n      this.$router.push('/login');\n    };\n\n    const toggleSide = () => {\n      this.isCollapse = !this.isCollapse;\n    };\n\n    const saveNavState = path => {\n      window.sessionStorage.setItem('activePath', path);\n      this.activePath = path;\n    };\n\n    return {\n      menulist,\n      isCollapse,\n      activePath,\n      logout,\n      toggleSide,\n      saveNavState\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsCA,eAAe;AACbA,OAAI,GAAK;AACP,UAAMC,QAAO,GAAI,CAAC;AAChBC,UAAI,EAAE,EADU;AAEhBC,UAAI,EAAE,iBAFU;AAGhBC,SAAG,EAAE,CAAC;AACJF,YAAI,EAAE,EADF;AAEJG,aAAK,EAAE,WAFH;AAGJC,YAAI,EAAE;AAHF,OAAD;AAHW,KAAD,EAQd;AACDJ,UAAI,EAAE,EADL;AAEDC,UAAI,EAAE,SAFL;AAGDC,SAAG,EAAE,CAAC;AACJF,YAAI,EAAE,EADF;AAEJG,aAAK,EAAE,cAFH;AAGJC,YAAI,EAAE;AAHF,OAAD,EAIF;AACDJ,YAAI,EAAE,EADL;AAEDG,aAAK,EAAE,cAFN;AAGDC,YAAI,EAAE;AAHL,OAJE;AAHJ,KARc,EAoBd;AACDJ,UAAI,EAAE,EADL;AAEDC,UAAI,EAAE,SAFL;AAGDC,SAAG,EAAE;AAHJ,KApBc,CAAjB;AAyBA,UAAMG,UAAS,GAAI,KAAnB;AACA,UAAMC,UAAS,GAAIC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,YAA9B,CAAnB;;AAEA,UAAMC,MAAK,GAAI,MAAM;AACnBH,YAAM,CAACC,cAAP,CAAsBG,KAAtB;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB,QAAlB;AACF,KAHA;;AAIA,UAAMC,UAAS,GAAI,MAAM;AACvB,WAAKT,UAAL,GAAkB,CAAC,KAAKA,UAAxB;AACF,KAFA;;AAGA,UAAMU,YAAW,GAAKX,IAAD,IAAU;AAC7BG,YAAM,CAACC,cAAP,CAAsB